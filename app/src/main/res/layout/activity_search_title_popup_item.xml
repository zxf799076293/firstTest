<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="@dimen/account_title_bar_height"
        android:orientation="horizontal"
        android:background="@color/white"
        android:gravity="center_vertical">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/search_titel_popup_item_img"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/title_bar_title_txt_size"
            android:textColor="@color/headline_tv_color"
            android:layout_marginLeft="@dimen/field_list_item_margin"
            android:text=""
            android:id="@+id/search_titel_popup_item_tv"/>
    </LinearLayout>

</LinearLayout>