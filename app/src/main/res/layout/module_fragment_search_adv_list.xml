<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/searchlist_all_relativelayout"
    android:background="@color/app_linearlayout_bg">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/advertising_tffs_width"
        android:orientation="vertical"
        android:visibility="gone"
        android:id="@+id/searchlist_status_bar_ll"
        android:background="@color/default_bluebg">
    </LinearLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_bar_back_layout_width_height"
        android:layout_below="@+id/searchlist_status_bar_ll"
        android:background="@color/white"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:id="@+id/relativelayout_top">
        <TextView
            android:layout_width="@dimen/title_bar_back_layout_width_height"
            android:layout_height="@dimen/title_bar_back_layout_width_height"
            android:id="@+id/search_area_backimg"
            android:background="@null"
            android:drawableLeft="@drawable/ic_returnbutton_normal_three"
            android:textSize="@dimen/title_bar_title_txt_size"
            android:visibility="visible"
            android:textColor="@color/white"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/title_bar_back_marginLeft"
            android:drawablePadding="@dimen/home_area_drop_marginleft"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_toRightOf="@+id/search_area_backimg"
            android:layout_toLeftOf="@+id/search_jump_map_img"
            android:layout_centerVertical="true">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/home_area_drop_height"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/app_backbtn_marginleft"
                android:paddingRight="@dimen/TableRow_text_marginleft"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:background="@drawable/searchlist_search_tv_bg"
                android:drawableLeft="@drawable/img_search"
                android:drawablePadding="@dimen/maintab_paddingtop"
                android:paddingLeft="@dimen/txt_padding_left"
                android:id="@+id/edit_search_search"
                android:singleLine="true"
                android:hint="@string/search_hint_txt"
                android:textColorHint="@color/field_chair_textcolor"
                android:textSize="@dimen/login_leftright_txt_size"
                android:textColor="@color/headline_tv_color"/>
        </LinearLayout>
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="@dimen/order_type_layoutwidth"
            android:paddingRight="@dimen/app_backbtn_marginleft"
            android:paddingLeft="@dimen/app_backbtn_marginleft"
            android:layout_marginLeft="@dimen/app_backbtn_marginleft"
            android:id="@+id/search_jump_map_img"
            android:layout_centerVertical="true"
            android:src="@drawable/ic_titlebar_normal_three"
            android:background="@color/white"
            android:visibility="visible"
            android:layout_alignParentRight="true"/>
    </RelativeLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_below="@+id/relativelayout_top"
        android:layout_above="@+id/linearlayout_check">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/fieldinfo_bt_marfinleft"
                android:layout_gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal">
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:id="@+id/Sequence"
                    android:layout_weight="1"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/search_Sequence_type_txt"
                        android:id="@+id/sort_txt"
                        android:textColor="@color/headline_tv_color"
                        android:textSize="@dimen/fieldinfo_diogle_time_textsize"
                        android:singleLine="true"
                        android:layout_centerInParent="true"
                        android:drawableRight="@drawable/ic_arrows_one_two_four_one"
                        android:drawablePadding="@dimen/TableRow_text_marginleft"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/app_backbtn_marginleft"
                    android:id="@+id/search_area_type_layout">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingLeft="@dimen/txt_padding"
                        android:text="@string/search_area_type_txt"
                        android:textColor="@color/headline_tv_color"
                        android:textSize="@dimen/fieldinfo_diogle_time_textsize"
                        android:drawableRight="@drawable/ic_open_gary_button_normal_three"
                        android:drawablePadding="@dimen/txt_padding"
                        android:singleLine="true"
                        android:id="@+id/search_area_type_textview"
                        android:layout_centerInParent="true"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:id="@+id/search_price_type_layout"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fieldinfo_specifications_pricetext"
                        android:textColor="@color/headline_tv_color"
                        android:textSize="@dimen/fieldinfo_diogle_time_textsize"
                        android:singleLine="true"
                        android:layout_centerInParent="true"
                        android:drawableRight="@drawable/ic_arrows_one_two_four_one"
                        android:drawablePadding="@dimen/TableRow_text_marginleft"
                        android:id="@+id/search_price_type_textview"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:id="@+id/price_lativelayout"
                    android:background="@color/white">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/screening_txt"
                        android:text="@string/screening_txt"
                        android:singleLine="true"
                        android:layout_centerInParent="true"
                        android:textColor="@color/headline_tv_color"
                        android:textSize="@dimen/fieldinfo_diogle_time_textsize"
                        android:drawableRight="@drawable/ic_screening"
                        android:drawablePadding="@dimen/TableRow_text_marginleft" />
                </RelativeLayout>
            </LinearLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/title_bar_linelayout_height"
                android:id="@+id/View_view"
                android:background="@color/new_viewbg"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="visible"
            android:id="@+id/searchlist_field_bable_ll">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/order_field_advertising_type_width"
                android:orientation="horizontal"
                android:background="@color/white"
                android:id="@+id/search_label_rg">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_marginLeft="@dimen/app_backbtn_marginleft"
                    android:gravity="center">
                    <CheckBox
                        android:layout_width="@dimen/diogle_view_btn_layout_height"
                        android:layout_height="@dimen/field_list_item_label_samll"
                        android:id="@+id/search_selfsupportshop_rbtn"
                        android:gravity="center"
                        android:text="@string/search_activity_label_cb_str"
                        android:textSize="@dimen/txt_title_size_info"
                        android:checked="false"
                        style="@style/SearchCheckboxStyle"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_marginLeft="@dimen/home_gridview_texttwo_marginleft"
                    android:gravity="center">
                    <CheckBox
                        android:layout_width="@dimen/user_info_account_width"
                        android:layout_height="@dimen/field_list_item_label_samll"
                        android:id="@+id/search_hotsell_rbtn"
                        android:gravity="center"
                        android:text=""
                        android:textSize="@dimen/txt_title_size_info"
                        android:checked="false"
                        style="@style/SearchCheckboxStyle"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_marginLeft="@dimen/home_gridview_texttwo_marginleft"
                    android:gravity="center">
                    <CheckBox
                        android:layout_width="@dimen/user_info_account_width"
                        android:layout_height="@dimen/field_list_item_label_samll"
                        android:id="@+id/search_new_res_cb"
                        android:gravity="center"
                        android:text="@string/search_new_res_label_str"
                        android:textSize="@dimen/txt_title_size_info"
                        android:checked="false"
                        style="@style/SearchCheckboxStyle"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_marginLeft="@dimen/home_gridview_texttwo_marginleft"
                    android:gravity="center">
                    <CheckBox
                        android:layout_width="@dimen/user_info_account_width"
                        android:layout_height="@dimen/field_list_item_label_samll"
                        android:id="@+id/search_bestsell_rbtn"
                        android:gravity="center"
                        android:text="@string/search_bestsell_label_str"
                        android:textSize="@dimen/txt_title_size_info"
                        android:checked="false"
                        style="@style/SearchCheckboxStyle"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center">
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/txt_padding_line"
                android:orientation="vertical"
                android:background="@color/new_viewbg">
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:visibility="visible">
            <LinearLayout
                android:id="@+id/lay_no_searchlist"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:gravity="center"
                android:visibility="gone"
                android:background="@color/app_linearlayout_bg">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_search_two_six"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/fieldinfo_fieldtitle_height"
                    android:text="@string/search_fieldlist_no_data_text"
                    android:lineSpacingExtra="@dimen/tab_item_img_margintop"
                    android:textSize="@dimen/login_leftright_txt_size"
                    android:textColor="@color/register_edit_color"/>
                <TextView
                    android:layout_width="@dimen/review_button_width"
                    android:layout_height="@dimen/title_bar_layout_width_height"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/fieldinfo_fieldtitle_height"
                    android:text="@string/search_fieldlist_post_demand_text"
                    android:textSize="@dimen/text_title_size_next"
                    android:id="@+id/search_add_demand_textview"
                    android:textColor="@color/top_title_center_txt_color"
                    android:background="@drawable/activity_searchfieldaarea_delete_history_bg"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/invoiceinfoentry_marginleft"
                android:layout_gravity="center_horizontal"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/app_backbtn_marginleft"
                android:paddingRight="@dimen/app_backbtn_marginleft"
                android:background="@color/app_samelinearlayout_bg"
                android:id="@+id/searchlist_itemsize_ll"
                android:visibility="gone">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/fieldinfo_chair_textsize"
                    android:textColor="@color/register_edit_color"
                    android:id="@+id/searchlist_itemsize_text"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text=""
                    android:visibility="visible"/>

            </LinearLayout>
            <android.support.v4.widget.SwipeRefreshLayout
                android:id="@+id/swipe_refresh"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                >
                <com.linhuiba.business.view.LoadMoreListView
                    android:id="@+id/order_list"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:divider="@null"
                    android:visibility="visible"
                    android:background="@color/app_linearlayout_bg" />
            </android.support.v4.widget.SwipeRefreshLayout>
        </LinearLayout>
    </LinearLayout>
    <View
        android:layout_width="@dimen/addshoppingcard_width"
        android:layout_height="@dimen/txt_padding_line"
        android:background="@color/transparent"
        android:layout_below="@id/relativelayout_top"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/app_backbtn_marginleft"
        android:layout_marginTop="-15dp"
        android:id="@+id/search_popup_view">
    </View>

</RelativeLayout>