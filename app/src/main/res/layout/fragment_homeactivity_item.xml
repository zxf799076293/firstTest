<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="wrap_content"
    android:gravity="center"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <View
        android:layout_width="@dimen/txt_margin"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:visibility="gone"
        android:id="@+id/home_service_provider_view">
    </View>
    <View
        android:layout_width="@dimen/advertising_margintop"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:visibility="gone"
        android:id="@+id/home_service_provider_cut_off_rule_view">
    </View>
    <LinearLayout
        android:layout_width="300dp"
        android:layout_height="200dp"
        android:orientation="vertical"
        android:visibility="gone"
        android:id="@+id/home_community_no_data_ll">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible"
            android:layout_marginBottom="@dimen/field_list_item_label_samll"
            android:id="@+id/home_community_no_data_imgv"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical"
        android:visibility="visible"
        android:id="@+id/home_community_all_ll">
        <RelativeLayout
            android:layout_width="169dp"
            android:layout_height="131dp"
            android:background="@color/white"
            android:id="@+id/image_layout"
            >
            <com.linhuiba.linhuifield.fieldview.OvalImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                app:round="1"
                android:id="@+id/home_activity_img"
                app:show_leftup="true"
                app:show_rightup="true"
                app:show_leftdown="true"
                app:show_rightdown="true" />
            <ImageView
                android:layout_width="@dimen/order_type_layoutwidth"
                android:layout_height="@dimen/field_info_margintop"
                android:id="@+id/home_activity_label_img"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:visibility="gone"
                android:layout_marginTop="@dimen/txt_padding_left"
                android:layout_marginRight="@dimen/txt_padding_left"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/field_info_margintop"
                android:orientation="horizontal"
                android:layout_alignParentBottom="true"
                android:gravity="center_vertical"
                android:id="@+id/project_type_layout"
                android:visibility="gone"
                android:background="@drawable/fragment_home_activity_timei_bg"
                android:paddingLeft="@dimen/app_backbtn_marginleft"
                android:paddingRight="@dimen/app_backbtn_marginleft">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:textColor="@color/white"
                    android:textSize="@dimen/field_info_receipt_size"
                    android:text="@string/commoditypay_item_data_text"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:textColor="@color/white"
                    android:layout_marginLeft="@dimen/advertising_margintop"
                    android:textSize="@dimen/field_info_receipt_size"
                    android:id="@+id/activitylistitem_start_date_text"
                    android:text="@string/fieldinfo_no_parameter_message"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:textColor="@color/white"
                    android:layout_marginLeft="@dimen/maintab_width_margintop"
                    android:textSize="@dimen/field_info_receipt_size"
                    android:text="@string/fieldinfo_activity_time_middle_text"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:textColor="@color/white"
                    android:id="@+id/activitylistitem_end_date_text"
                    android:layout_marginLeft="@dimen/maintab_width_margintop"
                    android:textSize="@dimen/field_info_receipt_size"
                    android:text="@string/fieldinfo_no_parameter_message"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="@dimen/user_info_photo_text_top"
                android:orientation="horizontal"
                android:layout_alignParentBottom="true"
                android:visibility="visible"
                android:gravity="center_vertical"
                android:layout_marginBottom="@dimen/txt_padding_left"
                android:layout_marginLeft="@dimen/txt_padding_left"
                android:id="@+id/home_service_provider_label_ll"
                android:layout_marginTop="@dimen/text_title_next_padding"
                android:paddingRight="@dimen/text_title_next_padding">
                <TextView
                    android:layout_width="@dimen/order_pay_width"
                    android:layout_height="@dimen/user_info_photo_text_top"
                    android:textColor="@color/white"
                    android:background="@drawable/module_fragment_home_service_provider_label_bg"
                    android:textSize="@dimen/fieldinfo_chair_textsize"
                    android:text="600"
                    android:gravity="center"
                    android:id="@+id/home_service_provider_label_one_tv"/>
                <TextView
                    android:layout_width="@dimen/order_pay_width"
                    android:layout_height="@dimen/user_info_photo_text_top"
                    android:background="@drawable/module_fragment_home_service_provider_label_bg"
                    android:textColor="@color/white"
                    android:gravity="center"
                    android:textSize="@dimen/fieldinfo_chair_textsize"
                    android:layout_marginLeft="@dimen/txt_margin"
                    android:text="600"
                    android:id="@+id/home_service_provider_label_two_tv"/>
            </LinearLayout>

        </RelativeLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/searchlist_item_evaluation_width"
            android:id="@+id/home_activity_typelayout"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/home_activity_iten_viewwidth"
                android:textSize="@dimen/text_title_size_next"
                android:textStyle="bold"
                android:textColor="@color/headline_tv_color"
                android:id="@+id/home_activity_name"
                android:singleLine="true"
                android:text=""/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="@dimen/user_info_photo_text_top"
                android:background="@drawable/fragment_home_hot_subsidy_fee_bg"
                android:orientation="horizontal"
                android:visibility="gone"
                android:gravity="center_vertical"
                android:layout_marginTop="@dimen/field_list_item_padding"
                android:paddingLeft="@dimen/text_title_next_padding"
                android:paddingRight="@dimen/text_title_next_padding"
                android:id="@+id/home_subsidy_ll">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/groupbooking_confirm_order_btn_bg"
                    android:textSize="@dimen/fieldinfo_chair_textsize"
                    android:text="600"
                    android:id="@+id/home_subsidy_tv"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="bottom"
                android:layout_marginTop="@dimen/tab_item_margintop">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom"
                    android:id="@+id/home_hot_item_price_ll"
                    android:orientation="horizontal">
                    <ImageView android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:id="@+id/home_community_subsidy_img"
                        android:visibility="visible"
                        android:layout_marginBottom="@dimen/advertising_margintop"
                        android:layout_marginRight="@dimen/txt_padding"
                        android:src="@drawable/ic_subsidy_normal_three_copy"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/login_leftright_txt_size"
                        android:textColor="@color/price_tv_color"
                        android:id="@+id/home_activity_price"
                        android:textStyle="bold"
                        android:singleLine="true"
                        android:text=""/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/login_leftright_txt_size"
                        android:id="@+id/home_activity_price_unit_tv"
                        android:layout_marginLeft="@dimen/app_line_view_one_height"
                        android:textColor="@color/orderconfirm_success_bottom_btntextcolor"
                        android:singleLine="true"
                        android:text="@string/fieldinfo_subsidy_fee_first_prompt"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="bottom"
                    android:layout_marginLeft="@dimen/home_gridview_texttwo_marginleft"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/txt_title_size_info"
                        android:textColor="@color/register_edit_color"
                        android:singleLine="true"
                        android:text="@string/module_home_order_size"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/txt_title_size_info"
                        android:textColor="@color/register_edit_color"
                        android:singleLine="true"
                        android:id="@+id/home_community_order_size_tv"
                        android:text=""/>
                </LinearLayout>

            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:textSize="@dimen/fieldinfo_diogle_time_textsize"
                android:textColor="@color/price_tv_color"
                android:textStyle="bold"
                android:text="@string/home_hot_inquiry_tv_str"
                android:layout_marginTop="@dimen/tab_item_img_margintop"
                android:id="@+id/home_hot_item_inquiry_tv"
                android:visibility="gone"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/add_lab_width"
            android:visibility="gone"
            android:id="@+id/home_service_provider_ll"
            android:orientation="vertical"
            android:background="@drawable/ic_yinyingone_threesixone"
            android:paddingLeft="@dimen/TableRow_text_marginleft"
            android:paddingRight="@dimen/TableRow_text_marginleft"
            android:paddingBottom="@dimen/field_info_height">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingLeft="@dimen/app_backbtn_marginleft"
                android:background="@color/white">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/tab_txt_size"
                    android:layout_marginTop="@dimen/home_area_drop_marginleft"
                    android:textColor="@color/home_item_sercvice_city_color"
                    android:id="@+id/home_service_provider_city_tv"
                    android:text=""/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/app_line_view_one_height"
                    android:textSize="@dimen/text_title_size_next"
                    android:textStyle="bold"
                    android:textColor="@color/headline_tv_color"
                    android:id="@+id/home_service_provider_name_tv"
                    android:singleLine="true"
                    android:text=""/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/app_line_view_one_height"
                    android:gravity="center_vertical"
                    android:clickable="true"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/register_edit_color"
                        android:textSize="@dimen/register_codebtn_txt_size"
                        android:text="@string/module_home_item_service_mobile"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/register_codebtn_txt_size"
                        android:textColor="@color/register_edit_color"
                        android:clickable="true"
                        android:id="@+id/home_service_provider_phone_tv"
                        android:text=""/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <View
        android:layout_width="@dimen/txt_margin"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:visibility="gone"
        android:id="@+id/home_service_provider_right_view">
    </View>

</LinearLayout>