<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:background="@color/white"
    android:gravity="center"
    android:layout_height="wrap_content"
    android:id="@+id/field_review_alllayout">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/field_review_layout"
        android:gravity="center"
        android:orientation="vertical"
        android:background="?android:attr/selectableItemBackground">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="@dimen/txt_padding_left"
            android:paddingBottom="@dimen/txt_padding_left"
            android:id="@+id/field_review_grade_layout"
            android:visibility="gone">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/TableRow_img_marginleft"
                android:orientation="horizontal"
                android:gravity="center_vertical">

                <TextView
                    android:layout_width="@dimen/regiter_btnlayout_width"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/app_backbtn_marginleft"
                    android:text="@string/review_score_text"
                    android:textColor="@color/top_title_center_txt_color"
                    android:textSize="@dimen/login_leftright_txt_size"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/title_bar_back_layout_width_height"
                    android:background="@color/white"
                    android:layout_marginLeft="@dimen/home_gridview_texttwo_marginleft"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/review_img_0"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_img_1"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_img_2"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_img_3"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_img_4"
                        android:background="@drawable/publish_review_img"/>

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/TableRow_img_marginleft"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="@dimen/regiter_btnlayout_width"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/app_backbtn_marginleft"
                    android:text="@string/search_fieldlist_number_of_people"
                    android:textColor="@color/top_title_center_txt_color"
                    android:textSize="@dimen/login_leftright_txt_size"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/title_bar_back_layout_width_height"
                    android:background="@color/white"
                    android:layout_marginLeft="@dimen/home_gridview_texttwo_marginleft"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/review_number_of_people_img_0"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_number_of_people_img_1"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_number_of_people_img_2"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_number_of_people_img_3"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_number_of_people_img_4"
                        android:background="@drawable/publish_review_img"/>

                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/TableRow_img_marginleft"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="@dimen/regiter_btnlayout_width"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/app_backbtn_marginleft"
                    android:text="@string/review_property_cooperate_text"
                    android:textColor="@color/top_title_center_txt_color"
                    android:textSize="@dimen/login_leftright_txt_size"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/title_bar_back_layout_width_height"
                    android:background="@color/white"
                    android:layout_marginLeft="@dimen/home_gridview_texttwo_marginleft"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/review_property_cooperate_img_0"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_property_cooperate_img_1"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_property_cooperate_img_2"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_property_cooperate_img_3"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_property_cooperate_img_4"
                        android:background="@drawable/publish_review_img"/>

                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/TableRow_img_marginleft"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="@dimen/regiter_btnlayout_width"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/app_backbtn_marginleft"
                    android:text="@string/review_user_participation_text"
                    android:textColor="@color/top_title_center_txt_color"
                    android:textSize="@dimen/login_leftright_txt_size"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/title_bar_back_layout_width_height"
                    android:background="@color/white"
                    android:layout_marginLeft="@dimen/home_gridview_texttwo_marginleft"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/review_user_participation_img_0"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_user_participation_img_1"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_user_participation_img_2"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_user_participation_img_3"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_user_participation_img_4"
                        android:background="@drawable/publish_review_img"/>

                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/TableRow_img_marginleft"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="@dimen/regiter_btnlayout_width"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/app_backbtn_marginleft"
                    android:text="@string/review_complete_target_text"
                    android:textColor="@color/top_title_center_txt_color"
                    android:textSize="@dimen/login_leftright_txt_size"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/title_bar_back_layout_width_height"
                    android:background="@color/white"
                    android:layout_marginLeft="@dimen/home_gridview_texttwo_marginleft"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/review_complete_target_img_0"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_complete_target_img_1"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_complete_target_img_2"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_complete_target_img_3"
                        android:background="@drawable/publish_review_img"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/txt_padding_left"
                        android:id="@+id/review_complete_target_img_4"
                        android:background="@drawable/publish_review_img"/>

                </LinearLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/txt_padding_left"
                android:background="@color/new_viewbg">
            </View>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingBottom="@dimen/app_backbtn_marginleft">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/app_backbtn_marginleft">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:singleLine="true"
                    android:layout_marginRight="@dimen/app_backbtn_marginleft"
                    android:text=""
                    android:id="@+id/field_user_txt"
                    android:textColor="@color/orderconfirm_success_bottom_btntextcolor"
                    android:textSize="@dimen/login_leftright_txt_size"
                    android:layout_marginLeft="@dimen/app_backbtn_marginleft"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageview_layout"
                    android:orientation="horizontal"
                    android:layout_marginRight="@dimen/app_backbtn_marginleft">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/score_img_one"
                        android:src="@drawable/ic_xingxing_normal"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/score_img_two"
                        android:src="@drawable/ic_xingxing_normal"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/score_img_three"
                        android:src="@drawable/ic_xingxing_normal"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/score_img_four"
                        android:src="@drawable/ic_xingxing_normal"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/score_img_five"
                        android:src="@drawable/ic_xingxing_normal"/>
                </LinearLayout>
            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/app_backbtn_marginleft"
                android:id="@+id/txt_time"
                android:text="2016-6-6"
                android:textColor="@color/register_edit_color"
                android:textSize="@dimen/register_codebtn_txt_size"
                android:layout_marginTop="@dimen/searchlist_item_txt_margintop" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/evaluation_txt"
                android:layout_marginTop="@dimen/searchlist_item_txt_margintop"
                android:layout_marginLeft="@dimen/title_bar_back_marginLeft"
                android:layout_marginRight="@dimen/app_backbtn_marginleft"
                android:textColor="@color/app_big_headline_tv_color"
                android:textSize="@dimen/title_bar_title_txt_size"
                android:text=""/>
        </LinearLayout>
        <com.linhuiba.business.view.MyGridview
            android:layout_width="match_parent"
            android:layout_height="@dimen/choosespecifications_pricetxtwidth"
            android:numColumns="4"
            android:paddingLeft="@dimen/txt_padding"
            android:paddingRight="@dimen/txt_padding"
            android:scrollbars="none"
            android:gravity="center"
            android:layout_marginTop="@dimen/orderlist_item_linearlayout_marginleft"
            android:id="@+id/review_imggridview"
            >

        </com.linhuiba.business.view.MyGridview>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:layout_marginLeft="@dimen/app_backbtn_marginleft"
            android:layout_marginRight="@dimen/app_backbtn_marginleft"
            android:layout_marginTop="@dimen/searchlist_item_txt_margintop">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_marginRight="@dimen/txt_margin"
                android:layout_marginTop="@dimen/tab_item_margintop"
                android:id="@+id/txt_number_of_people_ll">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/fieldinfo_number_of_people_text"
                    android:textColor="@color/register_edit_color"
                    android:textSize="@dimen/txt_title_size_info" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/txt_number_of_people"
                    android:singleLine="true"
                    android:text="@string/review_people_number_tv_str"
                    android:textColor="@color/register_edit_color"
                    android:textSize="@dimen/txt_title_size_info" />
            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/review_res_size_tv"
                android:singleLine="true"
                android:text="@string/review_people_number_tv_str"
                android:textColor="@color/register_edit_color"
                android:layout_marginRight="@dimen/txt_margin"
                android:textSize="@dimen/txt_title_size_info" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/review_res_industry_tv"
                android:singleLine="true"
                android:text="@string/review_people_number_tv_str"
                android:textColor="@color/register_edit_color"
                android:textSize="@dimen/txt_title_size_info" />
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/btn_margin_width"
            android:orientation="vertical"
            android:id="@+id/review_label_relativelayout">
        </RelativeLayout>
        <View
            android:layout_width="fill_parent"
            android:layout_height="@dimen/title_bar_linelayout_height"
            android:layout_marginTop="@dimen/userinfo_layout_margin"
            android:background="@color/app_samelinearlayout_bg"
            />

    </LinearLayout>

</LinearLayout>