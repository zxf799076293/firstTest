<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:background="@color/white"
    android:gravity="center"
    android:layout_height="wrap_content"
    android:id="@+id/field_review_alllayout">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/field_review_layout"
        android:gravity="center"
        android:orientation="vertical"
        android:background="?android:attr/selectableItemBackground">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:id="@+id/field_review_grade_layout"
            android:visibility="gone">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/advertising_tffs_height"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:id="@+id/comment_composite_score_tv"
                        android:textColor="@color/headline_tv_color"
                        android:textSize="@dimen/wallet_balance_textview_size"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/tab_item_img_margintop"
                        android:text="@string/module_comment_composite_score"
                        android:textColor="@color/orderconfirm_success_bottom_btntextcolor"
                        android:textSize="@dimen/txt_title_size_info"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="bottom">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:id="@+id/comment_item_propertymatching_tv"
                            android:textColor="@color/headline_tv_color"
                            android:textSize="@dimen/wallet_balance_textview_size"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="%"
                            android:textColor="@color/headline_tv_color"
                            android:textSize="@dimen/login_leftright_txt_size"/>

                    </LinearLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/tab_item_img_margintop"
                        android:text="@string/module_comment_propertymatching"
                        android:textColor="@color/orderconfirm_success_bottom_btntextcolor"
                        android:textSize="@dimen/txt_title_size_info"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="bottom">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:id="@+id/comment_item_goalcompletion_tv"
                            android:textColor="@color/headline_tv_color"
                            android:textSize="@dimen/wallet_balance_textview_size"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="%"
                            android:textColor="@color/headline_tv_color"
                            android:textSize="@dimen/login_leftright_txt_size"/>

                    </LinearLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/tab_item_img_margintop"
                        android:text="@string/module_comment_goalcompletion"
                        android:textColor="@color/orderconfirm_success_bottom_btntextcolor"
                        android:textSize="@dimen/txt_title_size_info"/>
                </LinearLayout>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/txt_padding_left"
                android:background="@color/new_viewbg">
            </View>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingBottom="@dimen/app_backbtn_marginleft">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_marginTop="@dimen/app_backbtn_marginleft">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:singleLine="true"
                    android:layout_marginRight="@dimen/app_backbtn_marginleft"
                    android:text=""
                    android:id="@+id/field_user_txt"
                    android:textColor="@color/headline_tv_color"
                    android:textSize="@dimen/login_leftright_txt_size"
                    android:layout_marginLeft="@dimen/app_backbtn_marginleft"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/txt_time"
                    android:text="2016-6-6"
                    android:textColor="@color/register_edit_color"
                    android:textSize="@dimen/register_codebtn_txt_size" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/searchlist_item_txt_margintop"
                android:orientation="horizontal"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/module_caseinfo_score"
                    android:layout_marginLeft="@dimen/app_backbtn_marginleft"
                    android:layout_marginRight="@dimen/txt_margin"
                    android:textColor="@color/register_edit_color"
                    android:textSize="@dimen/fieldinfo_chair_textsize" />
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageview_layout"
                    android:orientation="horizontal"
                    android:layout_marginRight="@dimen/app_backbtn_marginleft">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/score_img_one"
                        android:src="@drawable/ic_xingxing_normal"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/score_img_two"
                        android:src="@drawable/ic_xingxing_normal"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/score_img_three"
                        android:src="@drawable/ic_xingxing_normal"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/score_img_four"
                        android:src="@drawable/ic_xingxing_normal"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/score_img_five"
                        android:src="@drawable/ic_xingxing_normal"/>
                </LinearLayout>

            </LinearLayout>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/evaluation_txt"
                android:layout_marginTop="@dimen/searchlist_item_txt_margintop"
                android:layout_marginLeft="@dimen/title_bar_back_marginLeft"
                android:layout_marginRight="@dimen/app_backbtn_marginleft"
                android:textColor="@color/app_big_headline_tv_color"
                android:textSize="@dimen/title_bar_title_txt_size"
                android:text=""/>
        </LinearLayout>
        <com.linhuiba.business.view.MyGridview
            android:layout_width="match_parent"
            android:layout_height="@dimen/choosespecifications_pricetxtwidth"
            android:numColumns="4"
            android:paddingLeft="@dimen/txt_padding"
            android:paddingRight="@dimen/txt_padding"
            android:scrollbars="none"
            android:gravity="center"
            android:layout_marginTop="@dimen/orderlist_item_linearlayout_marginleft"
            android:id="@+id/review_imggridview">
        </com.linhuiba.business.view.MyGridview>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="@dimen/app_backbtn_marginleft"
            android:layout_marginLeft="@dimen/app_backbtn_marginleft"
            android:layout_marginRight="@dimen/app_backbtn_marginleft"
            android:paddingTop="@dimen/field_list_item_padding"
            android:paddingBottom="@dimen/field_list_item_padding"
            android:background="@drawable/activity_fieldinfo_show_all_resource_info_bg">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/comment_item_promotion_purpose_tv"
                    android:singleLine="true"
                    android:text="@string/module_comment_promotion_purpose"
                    android:textColor="@color/register_edit_color"
                    android:layout_marginRight="@dimen/myself_red_prompt_width"
                    android:textSize="@dimen/txt_title_size_info" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/comment_item_spread_way_tv"
                    android:singleLine="true"
                    android:layout_marginRight="@dimen/myself_red_prompt_width"
                    android:text="@string/module_comment_spread_way"
                    android:textColor="@color/register_edit_color"
                    android:textSize="@dimen/txt_title_size_info" />
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:layout_marginRight="@dimen/txt_margin"
                    android:id="@+id/txt_number_of_people_ll">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fieldinfo_number_of_people_text"
                        android:textColor="@color/register_edit_color"
                        android:textSize="@dimen/txt_title_size_info" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/txt_number_of_people"
                        android:singleLine="true"
                        android:text=""
                        android:textColor="@color/register_edit_color"
                        android:textSize="@dimen/txt_title_size_info" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/home_gridview_textone_marginleft">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/review_res_size_tv"
                    android:singleLine="true"
                    android:text="@string/review_people_number_tv_str"
                    android:textColor="@color/register_edit_color"
                    android:layout_marginRight="@dimen/txt_margin"
                    android:textSize="@dimen/txt_title_size_info" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/review_res_industry_tv"
                    android:layout_marginTop="@dimen/home_gridview_textone_marginleft"
                    android:singleLine="true"
                    android:text="@string/review_people_number_tv_str"
                    android:textColor="@color/register_edit_color"
                    android:textSize="@dimen/txt_title_size_info" />
            </LinearLayout>
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/btn_margin_width"
            android:orientation="vertical"
            android:id="@+id/review_label_relativelayout">
        </RelativeLayout>
        <View
            android:layout_width="fill_parent"
            android:layout_height="@dimen/title_bar_linelayout_height"
            android:layout_marginTop="@dimen/userinfo_layout_margin"
            android:background="@color/app_samelinearlayout_bg"
            />

    </LinearLayout>

</LinearLayout>