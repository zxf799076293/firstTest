<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_centerVertical="true"
        android:gravity="center"
        android:visibility="gone"
        android:id="@+id/app_defaylt_dialog_ll">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:gravity="center">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/activity_confirmorder_success_layout_bg"
                    android:orientation="vertical"
                    android:visibility="gone"
                    android:layout_marginLeft="@dimen/user_info_photo_text_top"
                    android:layout_marginRight="@dimen/user_info_photo_text_top"
                    android:id="@+id/app_defaylt_dialog_remind_ll">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:background="@drawable/activity_confirmorder_success_layout_bg">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textSize="@dimen/user_info_photo_txt_size"
                            android:textStyle="bold"
                            android:layout_marginLeft="@dimen/advertising_add_height"
                            android:layout_marginRight="@dimen/advertising_add_height"
                            android:layout_marginTop="@dimen/paddingleft"
                            android:layout_marginBottom="@dimen/paddingleft"
                            android:textColor="@color/headline_tv_color"
                            android:text=""
                            android:id="@+id/app_defaylt_title_tv"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:layout_marginBottom="@dimen/paddingleft"
                            android:id="@+id/app_defaylt_content_tv"
                            android:layout_marginLeft="@dimen/advertising_add_height"
                            android:layout_marginRight="@dimen/advertising_add_height"
                            android:textColor="@color/orderconfirm_success_bottom_btntextcolor"
                            android:textSize="@dimen/fieldinfo_diogle_time_textsize"
                            android:text="@string/fieldinfo_wallet_open_remind_text"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:background="@drawable/activity_confirmorder_success_layout_bg">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/user_info_account_width"
                            android:layout_marginLeft="@dimen/advertising_add_height"
                            android:layout_marginRight="@dimen/advertising_add_height"
                            android:layout_marginBottom="@dimen/paddingleft"
                            android:gravity="center"
                            android:background="@drawable/loginbtn_bg"
                            android:text="@string/wallet_open_cancel_btn_text"
                            android:textColor="@color/white"
                            android:textSize="@dimen/txt_title_size_nor"
                            android:visibility="gone"
                            android:id="@+id/app_default_one_btn_tv"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <View
                                style="@style/DefaultLongViewStyle">
                            </View>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/user_info_account_width"
                                android:orientation="horizontal">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="@string/wallet_open_cancel_btn_text"
                                    android:textColor="@color/orderconfirm_success_bottom_btntextcolor"
                                    android:textSize="@dimen/txt_title_size_nor"
                                    android:id="@+id/app_defaylt_cancel_tv"/>
                                <TextView
                                    android:layout_width="@dimen/txt_padding_line"
                                    android:layout_height="match_parent"
                                    android:background="@color/new_viewbg"
                                    android:layout_marginTop="@dimen/tab_item_margintop"
                                    android:layout_marginBottom="@dimen/tab_item_margintop"
                                    android:text=""/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:textColor="@color/default_bluebg"
                                    android:textSize="@dimen/txt_title_size_nor"
                                    android:text="@string/wallet_open_determine_btn_text"
                                    android:id="@+id/app_defaylt_confirm_tv"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/activity_confirmorder_success_layout_bg"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/field_list_item_label_samll"
                    android:paddingTop="@dimen/field_list_item_label_samll"
                    android:layout_marginLeft="@dimen/user_info_photo_text_top"
                    android:layout_marginRight="@dimen/user_info_photo_text_top"
                    android:visibility="gone"
                    android:id="@+id/app_defaylt_dialog_counselor_ll">
                    <include
                        layout="@layout/module_activity_fieldinfo_include_counselor">
                    </include>
                    <View
                        style="@style/DefaultLongViewStyle">
                    </View>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/edittext_height"
                        android:layout_marginLeft="@dimen/field_list_item_label_samll"
                        android:layout_marginRight="@dimen/field_list_item_label_samll"
                        android:layout_marginTop="@dimen/field_list_item_label_samll"
                        android:orientation="horizontal"
                        android:gravity="center_vertical"
                        android:id="@+id/app_defaylt_dialog_counselor_save_ll">
                        <ImageView
                            android:layout_width="@dimen/order_type_layoutwidth"
                            android:layout_height="@dimen/order_type_layoutwidth"
                            android:layout_marginLeft="@dimen/home_gridview_textone_marginleft"
                            android:id="@+id/app_defaylt_dialog_counselor_qrcode_imgv"
                            android:background="@drawable/ic_no_pic_small"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginLeft="@dimen/txt_margin">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/headline_tv_color"
                                android:textSize="@dimen/fieldinfo_chair_textsize"
                                android:text="@string/module_fieldinfo_counselor_dialog_save_wx_code_first"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/headline_tv_color"
                                android:layout_marginTop="@dimen/advertising_margintop"
                                android:textSize="@dimen/fieldinfo_chair_textsize"
                                android:text="@string/module_fieldinfo_counselor_dialog_save_wx_code_second"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/edittext_height"
                        android:layout_marginLeft="@dimen/field_list_item_label_samll"
                        android:layout_marginRight="@dimen/field_list_item_label_samll"
                        android:layout_marginTop="@dimen/app_backbtn_marginleft"
                        android:orientation="horizontal"
                        android:gravity="center_vertical"
                        android:id="@+id/app_defaylt_dialog_counselor_call_ll">
                        <ImageView
                            android:layout_width="@dimen/order_type_layoutwidth"
                            android:layout_height="@dimen/order_type_layoutwidth"
                            android:layout_marginLeft="@dimen/home_gridview_textone_marginleft"
                            android:id="@+id/app_defaylt_dialog_counselor_avatar_imgv"
                            android:background="@drawable/ic_no_pic_small"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginLeft="@dimen/txt_margin">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/headline_tv_color"
                                android:textSize="@dimen/fieldinfo_chair_textsize"
                                android:id="@+id/app_defaylt_dialog_counselor_call_tv"
                                android:text="呼叫电话"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/activity_confirmorder_success_layout_bg"
                    android:orientation="vertical"
                    android:layout_marginLeft="@dimen/user_info_photo_text_top"
                    android:layout_marginRight="@dimen/user_info_photo_text_top"
                    android:paddingTop="@dimen/field_list_item_label_samll"
                    android:visibility="gone"
                    android:gravity="center_horizontal"
                    android:id="@+id/fieldinfo_counselor_save_wx_code_ll">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/fieldinfo_counselor_save_wx_code_imgv"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/layout_margin_width"
                        android:textSize="@dimen/register_codebtn_txt_size"
                        android:textColor="@color/orderconfirm_success_bottom_btntextcolor"
                        android:text="@string/module_fieldinfo_counselor_save_wx"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginTop="@dimen/app_backbtn_marginleft"
                        android:id="@+id/fieldinfo_counselor_save_wx_code_btn_ll">
                        <View style="@style/DefaultLongViewStyle">
                        </View>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="@color/default_bluebg"
                            android:layout_marginTop="@dimen/app_backbtn_marginleft"
                            android:layout_marginBottom="@dimen/app_backbtn_marginleft"
                            android:textSize="@dimen/btnregister_txt_size"
                            android:text="@string/module_fieldinfo_counselor_save_wx_code"/>
                    </LinearLayout>
                </LinearLayout>
                <ImageButton
                    android:layout_width="@dimen/myself_order_layout_width"
                    android:layout_height="@dimen/myself_order_layout_width"
                    android:layout_marginTop="@dimen/home_gridview_texttwo_marginleft"
                    android:id="@+id/app_defaylt_close_img_btn"
                    android:background="@null"
                    android:visibility="gone"
                    android:src="@drawable/ic_close_two_four"/>
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/fieldinfo_all_dialog_layout"
        android:background="#98000000">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:gravity="center"
                android:paddingLeft="@dimen/fieldinfo_fieldtitle_height"
                android:paddingRight="@dimen/fieldinfo_fieldtitle_height">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/choose_calendar_gridview_height"
                    android:visibility="gone"
                    android:id="@+id/fieldinfo_refunt_price_detailed_layout">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingBottom="@dimen/order_type_layoutwidth"
                        android:background="@drawable/activity_confirmorder_success_layout_bg"
                        android:orientation="vertical"
                        android:gravity="center_horizontal"
                        android:layout_centerVertical="true"
                        android:id="@+id/refund_difference_price_layout">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/tab_item_img_margintop"
                            android:gravity="center_horizontal"
                            android:orientation="vertical">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/refunt_price_text"
                                android:layout_marginTop="@dimen/home_area_drop_height"
                                android:layout_gravity="center_horizontal"
                                android:textStyle="bold"
                                android:textColor="@color/app_big_headline_tv_color"
                                android:textSize="@dimen/user_info_photo_txt_size"
                                android:layout_centerHorizontal="true"
                                android:text="@string/fieldinfo_refunt_price_text"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginTop="@dimen/app_backbtn_marginleft"
                                android:layout_marginLeft="@dimen/invoiceinfoentry_marginleft"
                                android:layout_marginRight="@dimen/invoiceinfoentry_marginleft"
                                android:id="@+id/refunt_price_detailed"
                                android:textColor="@color/orderconfirm_success_bottom_btntextcolor"
                                android:lineSpacingExtra="@dimen/tab_item_margintop"
                                android:textSize="@dimen/txt_title_size_info"
                                android:text="@string/fieldinfo_refunt_price_detailedtext"/>
                        </LinearLayout>
                    </LinearLayout>
                    <ImageButton
                        android:layout_width="@dimen/fieldinfo_error_recovery_image_height"
                        android:layout_height="@dimen/fieldinfo_error_recovery_image_height"
                        android:layout_marginBottom="-42dp"
                        android:layout_centerHorizontal="true"
                        android:visibility="gone"
                        android:src="@drawable/ic_popup_logo_two_four_one"
                        android:layout_above="@+id/refund_difference_price_layout"
                        android:background="@drawable/activity_fieldinfo_error_correction_logo_bg"/>
                    <ImageButton
                        android:layout_width="@dimen/myself_order_layout_width"
                        android:layout_height="@dimen/myself_order_layout_width"
                        android:layout_centerHorizontal="true"
                        android:id="@+id/fieldinfo_explain_close_imgbtn"
                        android:layout_below="@+id/refund_difference_price_layout"
                        android:background="@null"
                        android:visibility="gone"
                        android:src="@drawable/ic_close_two_four"/>

                </RelativeLayout>
                <ImageButton
                    android:layout_width="@dimen/myself_order_layout_width"
                    android:layout_height="@dimen/myself_order_layout_width"
                    android:layout_marginTop="@dimen/btn_margin_width"
                    android:id="@+id/refunt_price_detailed_btn"
                    android:background="@null"
                    android:visibility="gone"
                    android:src="@drawable/ic_close_two_four"/>
            </LinearLayout>
        </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone"
            android:paddingLeft="@dimen/fieldinfo_fieldtitle_height"
            android:paddingRight="@dimen/fieldinfo_fieldtitle_height"
            android:id="@+id/fieldinfo_error_recovery_layout">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="368dp"
                android:background="@drawable/activity_confirmorder_success_layout_bg"
                android:orientation="vertical"
                android:gravity="center_horizontal"
                android:layout_centerInParent="true"
                android:id="@+id/fieldinfo_error_recovery_top_layout">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/home_area_drop_height"
                    android:layout_marginLeft="@dimen/home_area_drop_height"
                    android:layout_marginBottom="@dimen/field_list_item_label_samll"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/home_area_drop_height"
                        android:layout_gravity="center_horizontal"
                        android:textStyle="bold"
                        android:textColor="@color/app_big_headline_tv_color"
                        android:textSize="@dimen/user_info_photo_txt_size"
                        android:text="@string/fieldinfo_error_recovery_text"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/top_title_center_txt_color"
                        android:textSize="@dimen/txt_title_size_nor"
                        android:text="@string/fieldinfo_erro_correction_onetext"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.3"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.3"
                        android:orientation="vertical">
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/txt_padding_line"
                            android:background="@color/new_viewbg"
                            android:layout_marginLeft="@dimen/title_bar_back_marginLeft"
                            android:layout_marginRight="@dimen/title_bar_back_marginLeft"></View>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:layout_marginRight="@dimen/home_area_drop_height"
                            android:layout_marginLeft="@dimen/home_area_drop_height"
                            android:orientation="horizontal"
                            android:gravity="center_vertical">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:textColor="@color/title_bar_txtcolor"
                                android:textSize="@dimen/txt_title_size_nor"
                                android:text="@string/fieldinfo_error_recovery_resourcesname_text"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:textColor="@color/orderconfirm_success_bottom_btntextcolor"
                                android:textSize="@dimen/txt_title_size_nor"
                                android:singleLine="true"
                                android:id="@+id/error_recovery_resourcesname_text"
                                android:text="@string/fieldinfo_no_parameter_message"/>

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="2.8"
                            android:layout_marginRight="@dimen/home_area_drop_height"
                            android:layout_marginLeft="@dimen/home_area_drop_height"
                            android:orientation="vertical"
                            android:gravity="center_vertical">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:textColor="@color/title_bar_txtcolor"
                                android:textSize="@dimen/txt_title_size_nor"
                                android:text="@string/fieldinfo_error_recovery_content_text"/>
                            <EditText
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:id="@+id/error_recovery_content_edit"
                                android:layout_marginTop="@dimen/orderlist_item_linearlayout_marginleft"
                                android:paddingTop="@dimen/TableRow_text_marginleft"
                                android:layout_marginBottom="@dimen/TableRow_text_marginleft"
                                android:paddingLeft="@dimen/title_bar_back_marginLeft"
                                android:paddingRight="@dimen/title_bar_back_marginLeft"
                                android:gravity="top"
                                android:background="@color/tab_gb"
                                android:textColorHint="@color/divider_list"
                                android:hint="@string/fieldinfo_error_recovery_edit_hinttext"
                                android:textColor="@color/orderconfirm_success_bottom_btntextcolor"
                                android:textSize="@dimen/txt_title_size_nor"
                                android:maxLength="80"
                                android:ellipsize="end"
                                android:text=""/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="vertical">
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/txt_padding_line"
                                android:background="@color/new_viewbg"
                                android:layout_marginLeft="@dimen/myself_order_img_width"
                                android:layout_marginRight="@dimen/myself_order_img_width"></View>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="horizontal">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="@string/cancel"
                                    android:textColor="@color/orderconfirm_success_bottom_btntextcolor"
                                    android:textSize="@dimen/txt_title_size_nor"
                                    android:id="@+id/error_correction_cancel_text"/>
                                <TextView
                                    android:layout_width="@dimen/txt_padding_line"
                                    android:layout_height="match_parent"
                                    android:background="@color/new_viewbg"
                                    android:layout_marginTop="@dimen/tab_item_margintop"
                                    android:layout_marginBottom="@dimen/tab_item_margintop"
                                    android:text=""/>


                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:textColor="@color/default_bluebg"
                                    android:textSize="@dimen/txt_title_size_nor"
                                    android:text="@string/submit"
                                    android:id="@+id/error_correction_submit_text"/>

                            </LinearLayout>
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>
            <ImageButton
                android:layout_width="@dimen/fieldinfo_error_recovery_image_height"
                android:layout_height="@dimen/fieldinfo_error_recovery_image_height"
                android:visibility="gone"
                android:layout_marginBottom="-42dp"
                android:layout_centerHorizontal="true"
                android:src="@drawable/ic_popup_logo_two_four_one"
                android:layout_above="@+id/fieldinfo_error_recovery_top_layout"
                android:background="@drawable/activity_fieldinfo_error_correction_logo_bg"/>
        </RelativeLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_centerVertical="true"
            android:visibility="gone"
            android:gravity="center"
            android:id="@+id/wallet_remind_alllayout">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:gravity="center"
                android:paddingLeft="@dimen/fieldinfo_fieldtitle_height"
                android:paddingRight="@dimen/fieldinfo_fieldtitle_height">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:visibility="visible">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/home_area_drop_width"
                        android:paddingTop="@dimen/diogle_view_btn_height"
                        android:background="@drawable/activity_confirmorder_success_layout_bg"
                        android:orientation="vertical"
                        android:gravity="center_horizontal"
                        android:layout_centerVertical="true"
                        android:id="@+id/wallet_remind_layout">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="2.26"
                            android:gravity="center"
                            android:layout_marginLeft="@dimen/advertising_add_height"
                            android:layout_marginRight="@dimen/advertising_add_height"
                            android:textColor="@color/orderconfirm_success_bottom_btntextcolor"
                            android:textSize="@dimen/fieldinfo_diogle_time_textsize"
                            android:text="@string/fieldinfo_wallet_open_remind_text"/>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/txt_padding_line"
                            android:layout_marginLeft="@dimen/invoiceinfoentry_marginleft"
                            android:layout_marginRight="@dimen/invoiceinfoentry_marginleft"
                            android:background="@color/new_viewbg"></View>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="@string/wallet_open_cancel_btn_text"
                                android:textColor="@color/orderconfirm_success_bottom_btntextcolor"
                                android:textSize="@dimen/txt_title_size_nor"
                                android:id="@+id/wallet_open_cancel"/>
                            <TextView
                                android:layout_width="@dimen/txt_padding_line"
                                android:layout_height="match_parent"
                                android:background="@color/new_viewbg"
                                android:layout_marginTop="@dimen/tab_item_margintop"
                                android:layout_marginBottom="@dimen/tab_item_margintop"
                                android:text=""/>


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
