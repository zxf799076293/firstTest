<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/default_bluebg</item>//导航栏颜色
        <item name="colorPrimaryDark">@color/default_bluebg</item>//状态栏颜色
        <item name="colorAccent">@color/register_edit_color</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <!--<item name="windowActionBar">false</item>-->
        <!--<item name="windowNoTitle">true</item>-->
    </style>
    <style name="AppTheme.NoTitle_FullScreen">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="main_bar_text_style">
        <item name="android:textColor">@color/colorAccent</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="MyInvoiceCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">

        <item name="android:button">@drawable/applyforinvoice_listitem_checkedbg</item>
        <item name="android:textColor">@drawable/applyforinvoice_listitem_checkedtxtbg</item>
        <item name="android:background">@null</item>
    </style>
    <style name="MyorderCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/orderconfirm_listitem_checkedbg</item>
        <item name="android:textColor">@drawable/applyforinvoice_listitem_checkedtxtbg</item>
        <item name="android:background">@null</item>
    </style>
    <style name="AgreementCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/field_orderrefused_checkedbg</item>
        <item name="android:textColor">@drawable/applyforinvoice_listitem_checkedtxtbg</item>
        <item name="android:background">@null</item>
    </style>
    <style name="HomeReviewRemindCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/fragment_home_review_remind_checkbox_bg</item>
        <item name="android:background">@null</item>
    </style>

    <style name="TegisterCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/activity_register_agreement_checkbox_bg</item>
        <item name="android:textColor">@drawable/applyforinvoice_listitem_checkedtxtbg</item>
        <item name="android:background">@null</item>
    </style>

    <style name="appbottombtn_style">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_title_size_next</item>
        <item name="android:layout_marginLeft">@dimen/app_backbtn_marginleft</item>
        <item name="android:layout_marginRight">@dimen/app_backbtn_marginleft</item>
        <item name="android:background">@drawable/loginbtn_bg</item>
    </style>
    <style name="app_linearlayoutrighttxt_style">
        <item name="android:textColor">@color/top_title_center_txt_color</item>
        <item name="android:textSize">@dimen/txt_title_size_small</item>
        <item name="android:layout_marginLeft">@dimen/app_backbtn_marginleft</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>
    <style name="app_lefttextview_style">
        <item name="android:textColor">@color/top_title_center_txt_color</item>
        <item name="android:textSize">@dimen/txt_title_size_small</item>
        <item name="android:layout_marginLeft">@dimen/app_backbtn_marginleft</item>
    </style>

    <style name="app_linearlayoutrightedit_style">
        <item name="android:textColor">@color/orderconfirm_success_bottom_btntextcolor</item>
        <item name="android:textSize">@dimen/txt_title_size_small</item>
        <item name="android:paddingLeft">@dimen/orderlist_item_linearlayout_marginleft</item>
        <item name="android:textColorHint">@color/field_chair_textcolor</item>
        <item name="android:background">@color/white</item>
    </style>
    <style name="Theme" parent="android:Theme">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="CustomDialog">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="Willet_payment_ettor_dialog">
        <item name="android:textSize">@dimen/map_marker_textsize</item>
    </style>
    <style name="Bubble.TextAppearance.Dark" parent="android:TextAppearance.Medium">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#ff7f7f7f</item>
    </style>

    <style name="Bubble.TextAppearance.Light" parent="Bubble.TextAppearance.Dark">
        <item name="android:textColor">#ffeeeeee</item>
    </style>

    <style name="ClusterIcon.TextAppearance" parent="android:TextAppearance">
        <item name="android:textColor">#ffeeeeee</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TabLayoutTextStyle" parent="TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse">
        <item name="android:textSize">@dimen/login_leftright_txt_size</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="HomeTabLayoutTextStyle" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">@dimen/login_leftright_txt_size</item>
        <item name="textAllCaps">true</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="GroupTabLayoutTextStyle">
        <item name="android:textSize">@dimen/title_bar_title_txt_size</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="SearchCheckboxStyle" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@null</item>
        <item name="android:textColor">@drawable/activity_searchlist_screening_check_textview_bg</item>
        <item name="android:background">@drawable/activity_search_radiobtn_bg</item>
    </style>
    <style name="ApplyForInvoiceCheckboxStyle" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@null</item>
        <item name="android:textColor">@drawable/module_invoice_title_edit_choose_type_tv_color</item>
        <item name="android:background">@drawable/activity_invoice_checkbox_bg</item>
    </style>
    <style name="InvoiceInfoContentCheckboxStyle" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:textColor">@drawable/activity_invoice_checkbox_text_bg</item>
        <item name="android:button">@drawable/activity_invoiceinfo_content_checkbox_bg</item>
    </style>
    <style name="ApplyForInvoiceCChehooseResckboxStyle" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/applyforinvoice_listitem_checkedbg</item>
    </style>
    <style name="Theme.SplashTheme" parent="Theme.AppCompat.NoActionBar.FullScreen">
        <item name="android:windowBackground">@drawable/launch_three</item>
    </style>

    <style name="Theme.AppCompat.NoActionBar.FullScreen" parent="@style/Theme.AppCompat.NoActionBar">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="FieldinfoCheckboxStyle" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@null</item>
        <item name="android:textColor">@drawable/field_activity_childaccounteditor_checktext_bg</item>
        <item name="android:background">@drawable/activity_fieldinfo_srceen_checkbox_bg</item>
    </style>
    <style name="RegisterEditStyle">
        <item name="android:textColor">@color/headline_tv_color</item>
        <item name="android:textSize">@dimen/txt_title_size_nor</item>
        <item name="android:paddingLeft">@dimen/app_backbtn_marginleft</item>
        <item name="android:textColorHint">@color/register_hinttext_color</item>
        <item name="android:background">@color/white</item>
    </style>
    <style name="RegisterTabLayoutTextStyle">
        <item name="android:textSize">@dimen/title_bar_txtsize</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="RegisterBtnStyle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/user_info_photo_txt_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">@dimen/app_backbtn_marginleft</item>
        <item name="android:layout_marginRight">@dimen/app_backbtn_marginleft</item>
        <item name="android:background">@drawable/register_btn_unenable_bg</item>
    </style>
    <style name="InvoiceTitleCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/activity_invoiceinfo_content_checkbox_bg</item>
        <item name="android:textColor">@drawable/module_invoice_title_checkbox_textcolor</item>
        <item name="android:background">@null</item>
    </style>
    <style name="TabLayoutItemStyle" parent="android:Widget">
        <item name="tabBackground">@drawable/module_coupon_centre_tab_bg</item>
    </style>
    <style name="PublishReviewCheckBoxStyle" parent="android:Widget">
        <item name="android:background">@drawable/module_bg_publish_review_radiobutton</item>
        <item name="android:textColor">@drawable/module_bg_textcolor_white_gray</item>
    </style>
    <style name="PublishReviewGradeStyle" parent="android:Widget">
        <item name="android:background">@drawable/module_bg_publish_review_grade</item>
        <item name="android:textColor">@drawable/module_bg_textcolor_black_gray</item>
    </style>

</resources>
