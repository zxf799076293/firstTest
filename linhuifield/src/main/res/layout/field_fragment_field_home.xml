<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_linearlayout_bg"
    android:id="@+id/field_home_all_layout">
    <include
        layout="@layout/linhui_business_title_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <View
        android:layout_width="fill_parent"
        android:layout_height="@dimen/title_bar_linelayout_height"
        android:background="@color/top_title_line_color"/>
    <com.linhuiba.linhuifield.fieldview.Field_MyScrollview
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/field_home_scrollview">
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/app_linearlayout_bg"
                android:orientation="vertical"
                android:focusable="true"
                android:focusableInTouchMode="true">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >
                    <ImageView
                        android:id="@+id/field_home_image"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="visible"
                        android:spacing="1dp"
                        android:background="@drawable/ic_banner_houtai_two_five">
                    </ImageView>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical"
                    android:visibility="gone"
                    android:id="@+id/myenterprise_all_layout">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:visibility="gone"
                        android:id="@+id/field_fragment_myenterprise_layout">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/title_bar_back_layout_width_height"
                            android:visibility="visible"
                            android:background="@color/white">
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:orientation="horizontal"
                                android:gravity="center_vertical"
                                android:paddingLeft="@dimen/app_backbtn_marginleft"
                                android:layout_alignParentLeft="true">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/myselfinfo_term_management_text"
                                    android:textSize="@dimen/title_bar_title_txt_size"
                                    android:textColor="@color/headline_tv_color"
                                    />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:orientation="horizontal"
                                android:gravity="center_vertical"
                                android:paddingRight="@dimen/txt_margin"
                                android:layout_alignParentRight="true">
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_into_three"
                                    android:layout_marginLeft="@dimen/txt_padding"/>

                            </LinearLayout>
                        </RelativeLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/txt_padding_left"
                            android:background="@color/app_linearlayout_bg">
                        </View>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:visibility="gone"
                        android:id="@+id/field_fragment_enterprise_attestation_layout">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/title_bar_back_layout_width_height"
                            android:visibility="visible"
                            android:background="@color/white">
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:orientation="horizontal"
                                android:gravity="center_vertical"
                                android:paddingLeft="@dimen/app_backbtn_marginleft"
                                android:layout_alignParentLeft="true">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/enterprise_certification_text"
                                    android:textSize="@dimen/title_bar_title_txt_size"
                                    android:textColor="@color/headline_tv_color"
                                    />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:orientation="horizontal"
                                android:gravity="center_vertical"
                                android:id="@+id/next_ll"
                                android:paddingRight="@dimen/txt_margin"
                                android:layout_alignParentRight="true">
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_into_three"
                                    android:layout_marginLeft="@dimen/txt_padding"/>
                            </LinearLayout>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_toLeftOf="@+id/next_ll"
                                android:layout_marginLeft="@dimen/regiter_linelayout_height"
                                android:text="@string/myself_enterprise_certification_str"
                                android:textSize="@dimen/txt_title_size_info"
                                android:textColor="@color/register_edit_color"
                                />

                        </RelativeLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/txt_padding_left"
                            android:background="@color/app_linearlayout_bg">
                        </View>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/title_bar_layout_width_height"
                        android:orientation="vertical"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            style="@style/app_linearlayoutrighttxt_style"
                            android:text="@string/field_maintab_resource_management"/>
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/txt_padding_line"
                        android:background="@color/new_viewbg">
                    </View>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/home_gridview_img_width"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:id="@+id/field_fragment_field_layout">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/ic_space_normal_three"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/txt_padding"
                                android:text="@string/leftimg_text"
                                android:textColor="@color/top_title_center_txt_color"
                                android:textSize="@dimen/fieldinfo_diogle_time_textsize"/>
                        </LinearLayout>
                        <View
                            android:layout_width="@dimen/txt_padding_line"
                            android:layout_height="match_parent"
                            android:layout_marginTop="@dimen/field_info_marginleft"
                            android:layout_marginBottom="@dimen/field_info_marginleft"
                            android:background="@color/new_viewbg">
                        </View>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:id="@+id/field_fragment_activity_layout">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/ic_activity_normal_three"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/txt_padding"
                                android:text="@string/calendar_activity_txt"
                                android:textColor="@color/top_title_center_txt_color"
                                android:textSize="@dimen/fieldinfo_diogle_time_textsize"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical"
                    android:layout_marginTop="@dimen/txt_padding_left">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/title_bar_layout_width_height"
                        android:orientation="vertical"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            style="@style/app_linearlayoutrighttxt_style"
                            android:text="@string/field_home_more_functions_text"/>
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/txt_padding_line"
                        android:background="@color/new_viewbg">
                    </View>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/myself_order_layout_width"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:id="@+id/field_fragment_payment_account_layout">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/ic_receivable_two_five"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/home_area_drop_marginleft"
                                android:text="@string/applyforrelease_payment_account_txt"
                                android:textColor="@color/top_title_center_txt_color"
                                android:textSize="@dimen/txt_title_size_info"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:id="@+id/field_fragment_data_statistics_layout">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/ic_statistics_two_five"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/home_area_drop_marginleft"
                                android:text="@string/field_home_data_statistical_text"
                                android:textColor="@color/top_title_center_txt_color"
                                android:textSize="@dimen/txt_title_size_info"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:id="@+id/field_fragment_notice_guard_layout">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/ic_notice_two_five"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/home_area_drop_marginleft"
                                android:text="@string/txt_childaccount_titletxt"
                                android:textColor="@color/top_title_center_txt_color"
                                android:textSize="@dimen/txt_title_size_info"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:id="@+id/field_fragment_exclusive_layout">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/ic_exclusive_two_five"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/home_area_drop_marginleft"
                                android:text="@string/myselfinfo_exclusive_txt"
                                android:textColor="@color/top_title_center_txt_color"
                                android:textSize="@dimen/txt_title_size_info"/>
                        </LinearLayout>

                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginTop="@dimen/txt_padding_left"
                    android:visibility="visible"
                    android:id="@+id/property_home_switch_business_ll">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/fieldinfo_toleft_addshopcard"
                        android:visibility="visible"
                        android:background="@color/white">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal"
                            android:gravity="center_vertical"
                            android:paddingLeft="@dimen/app_backbtn_marginleft"
                            android:layout_alignParentLeft="true">
                            <ImageView
                                android:layout_width="@dimen/maintab_img_width_height"
                                android:layout_height="@dimen/maintab_img_width_height"
                                android:src="@drawable/ic_shangjia"
                                android:layout_centerVertical="true" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/app_backbtn_marginleft"
                                android:text="@string/myselfinfo_change_business_function_text"
                                android:textSize="@dimen/login_leftright_txt_size"
                                android:textColor="@color/top_title_center_txt_color"
                                />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal"
                            android:gravity="center_vertical"
                            android:paddingRight="@dimen/app_backbtn_marginleft"
                            android:layout_alignParentRight="true">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/ic_switch_two_five"
                                android:layout_marginLeft="@dimen/title_bar_back_marginBottom"/>

                        </LinearLayout>
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical"
                    android:layout_marginTop="@dimen/txt_padding_left"
                    android:visibility="gone"
                    android:id="@+id/field_home_neworder_remind_layout">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/title_bar_layout_width_height"
                        android:orientation="vertical"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            style="@style/app_linearlayoutrighttxt_style"
                            android:text="@string/field_home_neworder_remind_text"/>
                    </LinearLayout>
                    <com.linhuiba.linhuifield.fieldview.FieldMyListView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/app_linearlayout_bg"
                        android:divider="@null"
                        android:paddingBottom="@dimen/userinfo_layout_margin"
                        android:id="@+id/field_home_neworder_remind_listview">

                    </com.linhuiba.linhuifield.fieldview.FieldMyListView>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </com.linhuiba.linhuifield.fieldview.Field_MyScrollview>

</LinearLayout>